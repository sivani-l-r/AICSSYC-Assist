 {
     "builds": [
         {
             "src": "api/hello.py",
             "use": "@vercel/python"
         }
     ],
     "routes": [
         {
             "src": "/(.*)",
             "dest": "api/hello.py"
         }
     ],

  "rewrites": [{ "source": "/(.*)", "destination": "/" }]
 }
